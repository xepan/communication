#email-content.email-content-nano.has-scrollbar(style='height: 564px;')
  .email-content-nano-content(tabindex='0', style='right: -13px;')
    ul#email-list
      {rows}{row}
      li.clickable-row(data-href='email-detail.html')
        .chbox
          .checkbox-nice
            input#m-checkbox-1(type='checkbox')
            label(for='m-checkbox-1')
        .star
          a
        .name(style='overflow:hidden') {callback_from}Mila Kunis{/}
        .message
          span.label.label-success Work
          span.subject {callback_subject}Like your new movie -{/}&nbsp;
          span.body {description}Hi beauty, I watched your movies last night.{/}
        .meta-info
          a.attachment(href='#')
            i.fa.fa-paperclip
          span.date {callback_date}9:34 am{/}
      {/}{/}
  .nano-pane
    .nano-slider(style='height: 274px; transform: translate(0px, 0px);')    